<h2>Photos</h2>

<ul id="mosaic">
  <%= render @photos %>
</ul>

<% if can? :create, @photo %>
  <%= render 'form' %>
  <script id="template-upload" type="text/x-tmpl">
    <div class="upload">
      {%=o.name%}
      <div class="progress"><div class="bar" style="width: 0%"></div></div>
    </div>
  </script>
<% end %>
